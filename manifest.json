{
  "manifest_version": 3,
  "name": "Find Lite",
  "description": "Find Lite is a simple extension that allows you to search for text on the current page.",
  "version": "1.0.0",
  "web_accessible_resources": [
    {
      "resources": [
        "content/content.css",
        "icons/exit.png",
        "icons/up-enable.png",
        "icons/up-disable.png",
        "icons/down-enable.png",
        "icons/down-disable.png"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "app.js",
        "content/panel.js",
        "content/page.js",
        "content/finder.js",
        "content/listener.js",
        "content/content.js"
      ]
    }
  ]
}